{
	"name": "srp-articles-spa",
	"private": true,
	"version": "0.0.0",
	"type": "module",
	"scripts": {
		"dev": "vite",
		"build": "tsc && vite build",
		"preview": "vite preview",
		"lint:eslint": "eslint src --ext ts,tsx",
		"lint:eslint:fix": "eslint src --ext ts,tsx --fix",
		"format": "biome format .",
		"format:fix": "biome format . --write",
		"format:prettier": "prettier --check \"**/*.{ts,tsx,js,cjs,mjs,json,md,scss,yml,yaml}\"",
		"format:prettier:fix": "prettier --write \"**/*.{ts,tsx,js,cjs,mjs,json,md,scss,yml,yaml}\"",
		"lint:biome": "biome check .",
		"lint:biome:fix": "biome lint . --write && biome format . --write",
		"check": "npm run lint:eslint && npm run lint:biome && npm run build",
		"fix": "biome check . --write && eslint src --ext ts,tsx --fix",
		"verify": "npm run lint:eslint && npm run lint:biome && npm run build",
		"fix:all": "npm run fix && npm run verify"
	},
	"dependencies": {
		"@emotion/react": "^11.14.0",
		"@emotion/styled": "^11.14.1",
		"@mui/material": "^7.3.6",
		"react": "^19.2.0",
		"react-dom": "^19.2.0",
		"react-router-dom": "^7.11.0",
		"zod": "^4.3.4",
		"zustand": "^5.0.9"
	},
	"devDependencies": {
		"@biomejs/biome": "^2.3.10",
		"@eslint/js": "^9.39.1",
		"@types/node": "^24.10.1",
		"@types/react": "^19.2.5",
		"@types/react-dom": "^19.2.3",
		"@typescript-eslint/eslint-plugin": "^8.51.0",
		"@typescript-eslint/parser": "^8.51.0",
		"@vitejs/plugin-react": "^5.1.1",
		"eslint": "^9.39.2",
		"eslint-config-prettier": "^10.1.8",
		"eslint-plugin-prettier": "^5.5.4",
		"eslint-plugin-react": "^7.37.5",
		"eslint-plugin-react-hooks": "^7.0.1",
		"eslint-plugin-react-refresh": "^0.4.24",
		"globals": "^16.5.0",
		"husky": "^9.1.7",
		"lint-staged": "^16.2.7",
		"prettier": "^3.7.4",
		"sass": "^1.97.1",
		"typescript": "~5.9.3",
		"typescript-eslint": "^8.46.4",
		"vite": "^7.2.4"
	}
}
